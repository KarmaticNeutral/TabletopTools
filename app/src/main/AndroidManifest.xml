<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Table_Top_Gaming">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Table Top Gaming"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.Table_Top_Gaming.Game"
            android:parentActivityName="com.example.Table_Top_Gaming.LoadGameActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.Table_Top_Gaming.LoadGameActivity" /></activity>
        <activity
            android:name="com.example.Table_Top_Gaming.LoadGameActivity"
            android:parentActivityName="com.example.Table_Top_Gaming.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.Table_Top_Gaming.MainActivity" />
        </activity>
        <activity android:name="com.example.Table_Top_Gaming.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>